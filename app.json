{
  "expo": {
    "name": "songsProCleint",
    "slug": "songsProCleint",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon3.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/icon3.png",
      "resizeMode": "contain",
      "backgroundColor": "#080003"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon3.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kapil9711.songsProCleint",
      "permissions": [
        "NOTIFICATIONS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET",
        "WAKE_LOCK",
        "FOREGROUND_SERVICE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "taskManager": {
        "backgroundFetch": {
          "minimumInterval": 1,
          "stopOnTerminate": false,
          "startOnBoot": true
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon3.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#030c14"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "108257a2-4b8a-4d7c-a2c7-d988e19c549c"
      }
    }
  }
}
